{
  "name": "react-sortable-tree-theme-wcag-editor",
  "version": "0.0.20",
  "description": "Minimal theme for react-sortable-tree",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production TARGET=umd webpack --bail",
    "build:demo": "npm run clean:demo && cross-env NODE_ENV=production TARGET=demo webpack --bail",
    "clean": "rimraf dist",
    "clean:demo": "rimraf build",
    "start": "cross-env NODE_ENV=development TARGET=development webpack serve --open",
    "lint": "eslint .",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"**/*.{js,jsx,ts,tsx,css,scss}\"",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@nosferatu500/react-dnd-scrollzone": "^2.0.10",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.0",
    "react-dnd": "14.0.4",
    "react-dnd-html5-backend": "14.1.0",
    "react-sortable-tree-wcag-editor": "^4.5.5",
    "react-virtualized": "^9.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^21.2.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "json-loader": "^0.5.4",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dnd-test-backend": "^2.5.4",
    "react-dnd-touch-backend": "^0.3.17",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.1",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "typescript": "^5.1.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": [
    "react",
    "react-component"
  ]
}
